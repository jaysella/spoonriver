{
  "name": "spoonriver",
  "private": true,
  "description": "Home for Spoon River main data with character analysis",
  "version": "0.1.0",
  "contributors": [
    {
      "name": "Jay Sella",
      "email": "jay@jaysella.dev",
      "url": "https://jaysella.com"
    },
    {
      "name": "Oscar Lloyd"
    },
    {
      "name": "Nick Casertano"
    }
  ],
  "dependencies": {
    "gatsby": "^4.25.7",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-sass": "^2.5.1",
    "gatsby-plugin-sharp": "^2.8.0",
    "gatsby-source-filesystem": "^2.5.0",
    "gatsby-transformer-json": "^2.5.0",
    "gatsby-transformer-sharp": "^2.6.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.1",
    "prettier": "^2.0.5",
    "sass": "^1.29.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "See license in LICENSE",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "lint": "eslint './src/**/*.{js,jsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx}' --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{js,jsx,json,md}": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/j-651/spoonriver"
  },
  "bugs": {
    "url": "https://github.com/j-651/spoonriver/issues"
  }
}
